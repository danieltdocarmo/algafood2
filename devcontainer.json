{
  "name": "Java Dev Container",
  "image": "mcr.microsoft.com/devcontainers/java:17",
  "features": {
    "ghcr.io/devcontainers/features/java:1": {
      "version": "17"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "humao.rest-client"
      ]
    }
  },
  "postCreateCommand": "mvn -v"
}